    <string name="app_name">AudioTime</string>
    <string name="auto_range">\tToggle auto range of amplitude.</string>
    <string name="domain_label">time</string>
    <string name="fft">\tDetermine dominant frequency in window.</string>
    <string name="fft_title">FFT results</string>
    <string name="frequency">Frequency (Hz) of the four highest power: </string>
    <string name="help">\tHelp and information.</string>
    <string name="info">AudioTime can record, display audio events, and measure the relative time of occurrence between two audio events, and determine the dominant frequency of an audio segment.\n
						Gestures select graph areas:\n
						Tap one finger to mark the interval yellow.\n
						Drag the yellow interval mark.\n
						Two finger pinch or squeeze to zoom.\n
						Drag to pan left and right.\n
						Tap twice to remove yellow markers.\n
	</string>
    <string name="interval_label">Interval</string>
    <string name="left_side">\tLeft end of samples.</string>
    <string name="play_recording">\tPlay the windowed samples.</string>
    <string name="range_label">Amplitude</string>
    <string name="recommended">Recommended window minimum: </string>
    <string name="reset_graph">\tReset window to full recorded sample time.</string>
    <string name="right_side">\tRight end of samples.</string>
    <string name="samples">Samples: </string>
    <string name="save_recording">\tSave the windowed samples.</string>
    <string name="start_recording">\tStart recording samples.</string>
    <string name="stop_recording">\tStop recording.</string>
    <string name="window">Window: </string>
